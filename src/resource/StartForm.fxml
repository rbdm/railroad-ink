<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="comp1110.ass2.controller.StartFormController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="btn_NewGame" layoutX="231.0" layoutY="210.0" mnemonicParsing="false" onAction="#btn_NewGame_Click" text="New Game">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="btn_HighScore" layoutX="231.0" layoutY="281.0" mnemonicParsing="false" onAction="#btn_HighScore_Click" text="High Score">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <ComboBox fx:id="comboBox_PlayerNumber" layoutX="168.0" layoutY="98.0" prefHeight="23.0" prefWidth="305.0" promptText="Please select the number of Player" />
      </children>
    </AnchorPane>
  </children>
</VBox>
